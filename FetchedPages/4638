		<script type="text/javascript">
			toggleLocations = function() {
				var locationCount=parseInt(6);
				if(locationCount <= 10 ){
					$('#location_toggler').hide();
					$('#locationContainer').show();
				}
				jQuery('#location_toggler').toggle(
					function() {
						$('#locationContainer').toggle();
						$('#location_toggler').html('Hide Location');
					},
					function() {
						$('#locationContainer').toggle();
						$('#location_toggler').html('Show Location');
					}
				);
			}

			toggleJobCategories = function() {
				var jobCategoryCount=parseInt(13);
				if(jobCategoryCount <= 10 ){
					$('#job_category_toggler').hide();
					$('#jobCategoryContainer').show();
				}
				jQuery('#job_category_toggler').toggle(
					function() {
						$('#jobCategoryContainer').toggle();
						$('#job_category_toggler').html('Hide Job Category');
					},
					function() {
						$('#jobCategoryContainer').toggle();
						$('#job_category_toggler').html('Show Job Category');
					}
				);
			}
			
			togglejobOrganizations = function() {
				var jobOrganizationCount=parseInt(0);
				if(jobOrganizationCount <= 10 ){
					$('#job_organization_toggler').hide();
					$('#jobOrganizationContainer').show();
				}
				jQuery('#job_organization_toggler').toggle(
					function() {
						$('#jobOrganizationContainer').toggle();
						$('#job_organization_toggler').html('Hide Organization');
					},
					function() {
						$('#jobOrganizationContainer').toggle();
						$('#job_organization_toggler').html('Show Organization');
					}
				);
			}
			
			togglejobDepartments = function() {
				var jobDepartmentCount=parseInt(0);
				if(jobDepartmentCount <= 10 ){
					$('#job_department_toggler').hide();
					$('#jobDepartmentContainer').show();
				}
				jQuery('#job_department_toggler').toggle(
					function() {
						$('#jobDepartmentContainer').toggle();
						$('#job_department_toggler').html('Hide Department');
					},
					function() {
						$('#jobDepartmentContainer').toggle();
						$('#job_department_toggler').html('Show Department');
					}
				);
			}
			
			togglejobfunctions = function() {
				var jobFunctionCount=parseInt(21);
				if(jobFunctionCount <= 10 ){
					$('#job_function_toggler').hide();
					$('#jobFunctionContainer').show();
				}
				jQuery('#job_function_toggler').toggle(
					function() {
						$('#jobFunctionContainer').toggle();
						$('#job_function_toggler').html('Hide Job Functions');
					},
					function() {
						$('#jobFunctionContainer').toggle();
						$('#job_function_toggler').html('Show Job Functions');
					}
				);
			}
			
			clearSearch = function(){
				$('#start_close_date').val('');
				$('#end_close_date').val('');
				$('#title').val('');
				$(':checkbox:checked').each(function() {
					$(this).attr('checked',false);
				});
			}

			$(document).ready(function(){
				toggleLocations();
				toggleJobCategories();
				togglejobOrganizations();
				togglejobDepartments();
				togglejobfunctions();
			});
