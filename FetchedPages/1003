<script>
  const btn = document.querySelector('.open-sharesheet');
  
  //Check if supported
  if (navigator.share) {
    console.log('Sharesheet supported');
  } else {
    console.log('Sharesheet not supported');
    btn.style.display = 'none';
  }
  
  let shareData = {
    url: 'https://www.lens.uic.edu/uic-lens-vol-3',
  }
  
  btn.addEventListener('click', () => {
    navigator.share(shareData)
  });
</script>
</ul>

    </div>
  </div>
</div>

