<article id="post-0" class="bp_register type-bp_register post-0 page type-page status-publish hentry">
    <header class="entry-header">
        <h1 class="entry-title">Create an Account</h1>
    </header><!-- .entry-header -->

    <div class="entry-content">
        
<style>
  select:invalid {
    box-shadow: none;
  }
  
  .invalid {
    box-shadow: 0 0 3px 1px red !important;
  }
  
  label[for="signup_username"], #signup_username {
    display: none !important;
  }
  
  /* Profile Details section email field */
  #field_318, #field_318-1 {
    display: none !important;
  }
  
  /* Organisations text input */
  #field_2 {
    display: none;
  }
  
  /* Organisations dropdown */
  #field_2_select {
    max-width: 90%;
    margin-bottom: 10px;
  }
</style>

<script>
  jQuery(document).ready(function($) {
    
  });
</script>

<script>
  var memberOrganisations = ['Ameren','American Prairie Corridor','Applied Ecological Services','ArborMetrics Solutions','CNUC','Caesar Kleberg Wildlife Research Institute/Texas Native Seeds Program','Cardno','Cardno Nursery','Clearion Software','ComEd','Conservation Blueprint','Conservation Land Stewardship','Corteva Agriscience','Cypress Creek Renewables','DTE Energy','Dewberry','Duke Energy','East Central Energy','Enable Midstream Partners','Energy Resources Center','Environmental Incentives','Ernst Conservation Seeds','Evergy','FDOT','FHWA HQ','Florida Wildflower Foundation','Friends of Hackmatack NWR','Glenn Wilkins','Grow With Trees Company','IDNR - Mason Nursery','IVM Partners  Inc.','Idaho Transportation Department','KY Transportation Cabinet','Kleinschmidt Group','Midwest Invasive Plant Network','MoDOT','Monarch Joint Venture','NRECA','National Fish and Wildlife Foundation','NextEra Energy','NiSource','NiSource, Inc.','North Central IPM Center','ODOT','PDK Horticultural &amp; Invasive Plant Control','Pheasants Forever &amp; Quail Forever','Pollinator Partnership','Public Service Commission of Wisconsin','RES','Retired from CNUtility Consulting/Independent','Retired from Utility Vegetation Management','Stantec','The Bee &amp; Butterfly Habitat Fund','The Save Our Monarchs Foundation','USFWS - OH','UW Madison','University of Illinois at Chicago','University of Northern Iowa Tallgrass Prairie Center','W Trees Utility Vegetation Management Consulting, LLC','Wildlife Habitat Council','Wisconsin Department of Transportation',];
  
  jQuery(document).ready(function($) {
    function manuallyCheckEmail() {
      if (!$('#signup_email').val()) {
        $('#signup_email').attr('placeholder', 'Please enter your email address');
        $('#signup_email').focus();

        return false;
      }
      
      return true;
    }
    
    function manuallyCheckOrg() {
      var isFirefox = navigator.userAgent.toLowerCase().indexOf('firefox') > -1;
      
      if (!$('#field_2_select').children('option:selected').val() && isFirefox) {
        $('#field_2_select').addClass('invalid');

        return;
      }
        
      $('#field_2_select').removeClass('invalid');
    }
    
    $('#signup_form').submit(function() {
      manuallyCheckOrg();
      return manuallyCheckEmail();
    });
    
    $('#signup_submit').click(function() {
      manuallyCheckOrg();
      manuallyCheckEmail();
    });
    
    // Auto-populate Profile Details section email field
    $('#signup_email').on('keypress blur change', function(e) {
      $('#signup_username').val($('#signup_email').val());
      $('#field_318').val($('#signup_email').val());
    });
    
    // Prevent user entering only spaces in any field
    $('input').blur(function() {
      $(this).val($.trim($(this).val()));
      $(this).addClass('refresh').removeClass('refresh'); // force input to refresh appearance - Chrome bug with at least email input field
    });
    
    var orgsText = $('#field_2');
    orgsText.removeAttr('required');
    orgsText.attr('placeholder', 'Enter name of your organization');
    
    orgsText.before('<select required id="field_2_select" name="field_2_select"></select>');
    var orgsSelect = $('#field_2_select');
    
    orgsSelect.append(
      $('<option></option>').val('').html('Please choose')
    );
    $.each(memberOrganisations, function(index, org) {
      orgsSelect.append(
        $('<option></option>').val(org).html(org)
      );
    });
    orgsSelect.append(
      $('<option></option>').val('Other').html('Other')
    );
    
    orgsSelect.val(orgsText.val());
    
    orgsSelect.on('blur change', function() {
      manuallyCheckOrg();
      
      if ($(this).children('option:selected').val() === 'Other') {
        orgsText.val('');
        orgsText.attr('required', true);
        orgsText.show();
        orgsText.focus();
      } else {
        orgsText.removeAttr('required');
        orgsText.hide();
        orgsText.val($(this).children('option:selected').val());
      }
    });
  });
</script>

