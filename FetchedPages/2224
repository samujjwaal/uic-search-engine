            function checkboxOther(name_of_field) { 
                var elements = document.survey_7149445_0510T202844.elements[name_of_field]; var length = elements.length; 
                if(elements[length-1].checked) 
                { 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].disabled=false; 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].focus(); 
                } 
                else 
                { 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].disabled=true; 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].value=''; 
                } 
            } 
            function radioOther(name_of_field) 
            { 
                document.survey_7149445_0510T202844[name_of_field  + '_other'].disabled=false; 
                document.survey_7149445_0510T202844[name_of_field  + '_other'].focus(); 
            } 
            
            function radioNotOther(name_of_field) {
                var name = document.survey_7149445_0510T202844[name_of_field + '_other'];
                if(typeof(name) != 'undefined' && name != null){
                    document.survey_7149445_0510T202844[name_of_field + '_other'].disabled=true; 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].value=''; 
                }
            }
            
            function selectOther(name_of_field) {
                var elements = document.survey_7149445_0510T202844.elements[name_of_field]; 
                var length = elements.length;
                var selectedIndex =
                document.survey_7149445_0510T202844.elements[name_of_field].selectedIndex; 
                if((length-1)==selectedIndex) 
                {
                    document.survey_7149445_0510T202844[name_of_field + '_other'].disabled=false; 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].focus(); 
                } 
                else 
                { 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].disabled=true; 
                    document.survey_7149445_0510T202844[name_of_field + '_other'].value=''; 
                } 
            }
            
            function checkBrowserVersion() {
            var rv = -1; 
            if (navigator.appName == 'Microsoft Internet Explorer') 
            {
            var ua = navigator.userAgent;
            var re = new RegExp("MSIE ([0-9]{1,}[.0-9]{0,})");
            if (re.exec(ua) != null)
            {
            rv = parseFloat(RegExp.$1);
            }    
            }
            if (rv >= 8.0)
            {
            //alert('is IE8');
            return true;
            }
            else
            {
            //alert('is not IE8');
            return false;
            }
            }
            
            //execute on page load if IE8 or above
            var isIE8orAbove = checkBrowserVersion();
            
            if(isIE8orAbove)
            {
            var wrapperWidth = document.getElementById('form-wrapper').offsetWidth; 
            
            for(var i=0;i < 300; i++){
            var name = "legend" + i;
            var obj = document.getElementById(name);
            if(obj != null)
            {
            wrapperWidth = wrapperWidth - 10;
            obj.style.width = "" + wrapperWidth + "px";
            }
            }
            }

            function ws_recaptcha_set_timeout()
            {
                setTimeout('ws_recaptcha_call_switch_type()', 3000);
            }
            
            function ws_recaptcha_call_switch_type()
            {
                Recaptcha.switch_type('audio');
            }
            
            var wsRecaptcha=document.getElementById('recaptcha_switch_audio_btn');
