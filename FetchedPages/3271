	<script>
		$(window).scroll(function() {
			if($(window).scrollTop() <= 100) {
				// $('nav #logo').css('display', 'none');
				$('nav #logo').removeClass('logoplus');
				$('nav #logo').addClass('logoless');
				$('div.top-section img.big-logo').css('display', 'unset');	
				
			} else {
				// $('nav #logo').css('display', 'inline-block');
				$('nav #logo').removeClass('logoless');
				$('nav #logo').addClass('logoplus');
				$('div.top-section img.big-logo').css('display', 'none');
			}
		});
		$(window).load(function() {
		    $('nav #logo').addClass('logoless');
		});
	</script>

            
        <script>
        	$(window).load(function() {
		    
		    paceOptions = {
			  ajax: false, // disabled
			  document: false, // disabled
			  eventLag: false, // disabled
			  elements: {
			    selectors: ['body.home']
			  }
			};
		    
		    Pace.on('done', function() {
		    	$('#red').delay(1000).fadeOut(800);
		    	
		    	$("html, body").delay(2500).animate({scrollTop: $('.block').offset().top + $('.block').outerHeight(true) - 80 }, 800);
  				
  				$(window).scroll($.debounce( 250, function(){
				    $('nav').switchClass('nav-up', 'nav-down');
				}));
		    	
		    });
		});
		</script>

