          <div class="intro-text">

                      <h1 class="_title post-title">Campus Housing Coronavirus Updates</h1>
              
            
              
    
        
  </div>
        <div class="components">

  
    
      

<section class="component  ">

  
  
    <div class="
    component-call-to-action
    component-call-to-action--dark
    
    ">

    
    <div class="component-description">
                    <h2 id="information-from-our-partners-in-the-office-of-student-affairs-and-the-dean-of-students-office" class="__title visible">Information from our partners in the Office of Student Affairs and the Dean of Students Office</h2>
                    </div>

    <ul class="_items call-to-action-links">
              <li>
                      <a  href="https://housing.uic.edu/campus-housing-coronavirus-updates/new-guidance-march-20-2020-a-letter-from-rex-tolliver-vice-chancellor-for-student-affairs/"
            target="_self">
                            <span class="call-to-action-links__icon call-to-action-links__icon--dark text-icon" aria-hidden="true">
