            function checkboxOther(name_of_field) { 
                var elements = document.survey_7074059_0510T200424.elements[name_of_field]; var length = elements.length; 
                if(elements[length-1].checked) 
                { 
                    document.survey_7074059_0510T200424[name_of_field + '_other'].disabled=false; 
                    document.survey_7074059_0510T200424[name_of_field + '_other'].focus(); 
                } 
                else 
                { 
                    document.survey_7074059_0510T200424[name_of_field + '_other'].disabled=true; 
                    document.survey_7074059_0510T200424[name_of_field + '_other'].value=''; 
                } 
            } 
            function radioOther(name_of_field) 
            { 
                document.survey_7074059_0510T200424[name_of_field  + '_other'].disabled=false; 
                document.survey_7074059_0510T200424[name_of_field  + '_other'].focus(); 
            } 
            
            function radioNotOther(name_of_field) {
                var name = document.survey_7074059_0510T200424[name_of_field + '_other'];
                if(typeof(name) != 'undefined' && name != null){
                    document.survey_7074059_0510T200424[name_of_field + '_other'].disabled=true; 
                    document.survey_7074059_0510T200424[name_of_field + '_other'].value=''; 
                }
            }
            
            function selectOther(name_of_field) {
                var elements = document.survey_7074059_0510T200424.elements[name_of_field]; 
