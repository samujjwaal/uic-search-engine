if(jQuery("#last-crumb").hasClass("final-breadcrumb-no-children")) { // has no children
	jQuery( "#block-menu-block-1" ).insertBefore( ".breadcrumb-actual-last" );
	jQuery( "#block-menu-block-2" ).insertAfter( ".breadcrumb-1" );
	jQuery('.breadcrumb-2').hide();
	jQuery( "#block-menu-block-4" ).insertAfter( "#block-menu-block-2");
	jQuery('.breadcrumb-3').hide();
	jQuery('.breadcrumb-last').hide();
	var $widthvar = jQuery("#block-menu-block-2").width() + 30;
	
	jQuery("#block-menu-block-4").css("margin-left", $widthvar);

	var $widthvar2 = jQuery("#block-menu-block-4").width() + 30;
	$widthvar2 = $widthvar2 + $widthvar;
	jQuery("#block-menu-block-1").css("margin-left", $widthvar2);

	var $widthvar3 = $widthvar2 + 30 + jQuery("#block-menu-block-1").width();
	jQuery("#last-crumb").css("margin-left", $widthvar3);

}else if(jQuery("#last-crumb").hasClass("more-children")) { // more than children, has grandchildren 
	jQuery( "#block-menu-block-1" ).insertBefore( ".breadcrumb-actual-last" );
	jQuery( "#block-menu-block-2" ).hide();
	jQuery( "#block-menu-block-4" ).hide();
	jQuery('.breadcrumb-2').hide();
	jQuery( "#block-menu-block-3" ).insertAfter( ".breadcrumb-actual-last" );
	jQuery('.breadcrumb-last').hide();

	var $widthvar = jQuery("#block-menu-block-1").width() + 30;
	jQuery("#block-menu-block-3").css("margin-left", $widthvar);


	//if page has children but not grandchildren (im an asshole, what is this?)
	if(jQuery("#block-menu-block-3").length === 0) {
		jQuery("#last-crumb").show();
		jQuery("#last-crumb").css("margin-left", $widthvar);
	}else if(jQuery("#block-menu-block-3").length > 0) {
		jQuery("#last-crumb").hide();
	}
}else{ //just has the children
	jQuery( "#block-menu-block-1" ).insertBefore( ".breadcrumb-actual-last" );
	jQuery( "#block-menu-block-1" ).css("color","");
	jQuery( "#block-menu-block-2" ).insertAfter( ".breadcrumb-1" );
	jQuery( "#block-menu-block-3" ).insertAfter( ".breadcrumb-actual-last" );
	jQuery( "#block-menu-block-4" ).hide();
	jQuery('.breadcrumb-last').hide();
	jQuery('.breadcrumb-2').hide();
	var $widthvar = jQuery("#block-menu-block-1").width() + 30;
	var $widthvar2 = jQuery("#block-menu-block-2").width() + 30;
	$widthvar = $widthvar + $widthvar2;
	jQuery("#block-menu-block-1").css("margin-left", $widthvar2);
	if(jQuery("#block-menu-block-3").length === 0) {
		jQuery("#last-crumb").css("margin-left", $widthvar);
	}else{
		jQuery("#last-crumb").hide();
	}
	jQuery("#block-menu-block-3").css("margin-left", $widthvar);
	//jQuery( "#block-menu-block-2" ).insertAfter( ".breadcrumb-last" );
}
