    <script>
        $(document).ready(function () {

            result = 0;

            $('#search-btn').click(function () {

                search();

            });

            $('#archive-search').on('keydown keyup', debounce(function (e) {
                if (e.which == 13) {
                    search();
                }
            }, 100));

            $('#reset-btn').click(function () {

                resetList();

            });

            $('#inline-search-reset').click(function () {

                resetList();

            });

            $('body').on('keydown keyup', function (e) {
                if (e.which == 27) {
                    resetList();
                }
            });

        });

        function resetList(mode = 'reset') {

            result = 0;
            $('.row.results').hide();
            $('.org-wrapper').show();
            $('.row.college-archive').show();
            $('.row.support-program-archive').show();
            $('.org-wrapper .video-collection .video-wrapper').show();
            // $('.row.video-wrapper').show();
            $('#result-count').html(0);
            if (mode != 'search') {

                $('#archive-search').val('');

            }

        }

        function search() {

            query = $('#archive-search').val();

            // Temp reset
            resetList('search');
            $('.org-wrapper').hide();
            collegeShowing = [];
            departmentShowing = [];

            // $('.row.video-wrapper').each(function (index, feature) {
            $('.org-wrapper .video-collection .video-wrapper').each(function (index, feature) {

                // todo - remove
                // console.log(index);
                // console.log('Query:');
                // console.log(query);
                // console.log('Feature:');
                // console.log(feature);

                var college = $(feature).data('college');
                var department = $(feature).data('department');
                var sponsor = $(feature).data('sponsor');
                var title = $(feature).data('title');

                // todo - remove
                // console.log('college:');
                // console.log(college);
                // console.log('department:');
                // console.log(department);
                // console.log('sponsor:');
                // console.log(sponsor);
                // console.log('title:');
                // console.log(title);

                // Check query against Title, College, and Sponsor
                if (
                    college.toLowerCase().indexOf(query.toString().toLowerCase()) >= 0 ||
                    department.toLowerCase().indexOf(query.toString().toLowerCase()) >= 0 ||
                    sponsor.toLowerCase().indexOf(query.toString().toLowerCase()) >= 0 ||
                    title.toLowerCase().indexOf(query.toString().toLowerCase()) >= 0
                ) {

                    // todo - remove
                    // console.log('Feature SHOWING:');
                    // console.log(feature);
                    // console.log( $(this).parent().parent());
                    // console.log($(this).parent().parent().data());


                    $(this).show();
                    $(this).parent().parent().show();
                    $(this).parent().parent().parent().show();
                    $(this).parent().parent().parent().parent().show();
                    $(this).parent().parent().parent().parent().parent().show();
                    if ($(this).parent().parent().data('collegeCd')) {

                        collegeShowing.push($(this).parent().parent().data('collegeCd'));

                    }

                    if ($(this).parent().parent().data('departmentId')) {

                        departmentShowing.push($(this).parent().parent().data('departmentId'));

                    }

                    result++;

                } else {

                    // $(this).hide();

                    if (collegeShowing.includes($(this).parent().parent().data('collegeCd')) || departmentShowing.includes($(this).parent().parent().data('departmentId'))) {

                        // do nothing aka. Don't hide it since we have a hit on a child feature

                    } else {

                        // $(this).parent().parent().parent().parent().hide();

                    }

                }

            });

            // todo - remove
            // console.log('Showing what colleges?');
            // console.log(collegeShowing);
            // console.log(departmentShowing);

            $('.row.results').show();
            $('#result-count').html(result);

        }

        //http://davidwalsh.name/javascript-debounce-function
        function debounce(func, wait, immediate) {
            var timeout;
            return function () {
                var context = this, args = arguments;
                var later = function () {
                    timeout = null;
                    if (!immediate) func.apply(context, args);
                };
                var callNow = immediate && !timeout;
                clearTimeout(timeout);
                timeout = setTimeout(later, wait);
                if (callNow) func.apply(context, args);
            };
        };

    </script>
</main>
