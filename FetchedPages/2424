            function linkDelete(linkId) 
            {
                var link = document.getElementById(linkId);
                var parent = link.parentNode;
                parent.removeChild(link); 
            }
            function linkReplace(linkFileName, linkId)
            {
                var oldLink = document.getElementById(linkId);
                var newLink = document.createElement('link');
                newLink.rel = 'stylesheet';
                newLink.type = 'text/css';
                newLink.href = linkFileName;
                newLink.setAttribute('id', linkId);
                var parent = oldLink.parentNode;
                parent.insertBefore(newLink, oldLink);
                parent.removeChild(oldLink);
            }
            function linkNew(linkFileName, linkId) 
            {
                var newlink = document.createElement("link");
                newlink.rel="stylesheet";
                newlink.type="text/css";
                newlink.href=linkFileName;
                newlink.setAttribute("id", linkId);
                var dskCont = document.getElementById('dskCont');
                var formEl = dskCont.parentNode;
                formEl.insertBefore(newlink, dskCont);
            }
            function jsUpdateSize()
            {
                var isResponsive;
                if (typeof(added_by_webtools_is_responsive) != 'undefined' && added_by_webtools_is_responsive != null)
                {
                    isResponsive = true;
                }
                else
                {
                    isResponsive = false;
                }
                
                deviceSize = document.body.offsetWidth;
                var contDiv= document.getElementById("contDiv");
                var contentWidth = contDiv.clientWidth;
                if(mode==='firstTime')
                {
                    if((deviceSize < contentMinWidthToMobile+1) || (contentWidth < (contentMinWidthToMobile+1)))
                    {
                        mode='mobile';
                        if(navigationLinks.length > 0)
                        {
                            //document.getElementById('mobile-nav').style.display = 'block';
                        }
                        var desktopWrapper = document.getElementById("desktop-wrapper");
                        var mobileWrapper = document.getElementById("mobile-wrapper");
                        var mobileContent = document.getElementById("mobile-content");
                        linkNew('https://webservices.illinois.edu/webservices/styles/app/blog/640.css','link_skin_application');
                        if (typeof(document.getElementById('nav-menu')) != 'undefined' && document.getElementById('nav-menu') != null)
                        {
                            if (typeof(appUiColor) != 'undefined' && appUiColor != null)
                            {
                                document.getElementById('nav-menu').style.backgroundColor = appUiColor;
                            }
                        }
                        var sharebarElements = document.querySelectorAll('.share-bar');
                        for(var index = 0, length = sharebarElements.length; index < length; index++)
                        {
                            var sharebar = sharebarElements[index];
                            sharebar.style.backgroundColor = contentLinkColor;
                        }
                        //
                        if(isResponsive == false)
                        {
                            if (typeof(desktopWrapper) != 'undefined' && desktopWrapper != null)
                            {
                                mobileContent.appendChild(contDiv);
                                desktopWrapper.style.display="none";
                                mobileWrapper.style.display="block";
                            }
                        }
                        var navMenu = document.getElementById('nav-menu');
                        var navMenuClass = navMenu.className;
                        var mobileMenu = document.getElementById('mobile-nav');                        
                        var mobileMenuClass = mobileMenu.className;
                        //
                        if(navMenuClass.indexOf('active') > -1)
                        {                                        
                            navMenu.className = navMenuClass.substring(0, navMenuClass.indexOf('active') - 1);
                            mobileMenu.className = mobileMenuClass.substring(0, mobileMenuClass.indexOf('active') - 1);
                        }
                    }
                    else
                    {
                        mode='desktop';
                        linkNew('https://webservices.illinois.edu/webservices/styles/app/blog/sample481_plus.css','link_skin_application');
                    }
                }
                else if((deviceSize < contentMinWidthToMobile+1) || (contentWidth < (contentMinWidthToMobile+1) && mode!=="mobile"))
                {
                    mode='mobile';
                    if(navigationLinks.length > 0)
                    {
                        //document.getElementById('mobile-nav').style.display = 'block';
                    }
                    linkReplace('https://webservices.illinois.edu/webservices/styles/app/blog/640.css','link_skin_application');
                    if (typeof(document.getElementById('nav-menu')) != 'undefined' && document.getElementById('nav-menu') != null)
                    {
                        if (typeof(appUiColor) != 'undefined' && appUiColor != null)
                        {
                            document.getElementById('nav-menu').style.backgroundColor = appUiColor;
                        }
                    }
                    var sharebarElements = document.querySelectorAll('.share-bar');
                    for(var index = 0, length = sharebarElements.length; index < length; index++)
                    {
                        var sharebar = sharebarElements[index];
                        sharebar.style.backgroundColor = contentLinkColor;
                    }
                    //
                    var dskCont = document.getElementById("dskCont");
                    var mobileContent = document.getElementById("mobile-content");
                    var mobileWrapper = document.getElementById("mobile-wrapper");
                    var desktopWrapper = document.getElementById("desktop-wrapper");
                
                    if(isResponsive == false)
                    {
                        if (typeof(desktopWrapper) != 'undefined' && desktopWrapper != null)
                        {
                            mobileContent.appendChild(contDiv);
                            desktopWrapper.style.display="none";
                            mobileWrapper.style.display="block";
                            mobileMaxWidthToDesktop=window.innerWidth;
                        }
                    }
                
                    var navMenu = document.getElementById('nav-menu');
                    var navMenuClass = navMenu.className;
                    
                    var mobileMenu = document.getElementById('mobile-nav');                        
                    var mobileMenuClass = mobileMenu.className;
                
                    if(navMenuClass.indexOf('active') > -1)
                    {                                        
                        navMenu.className = navMenuClass.substring(0, navMenuClass.indexOf('active') - 1);
                        mobileMenu.className = mobileMenuClass.substring(0, mobileMenuClass.indexOf('active') - 1);
                    }
                }
                else if(contentWidth > mobileMaxWidthToDesktop && mode!=="desktop")
                {
                    mode='desktop';
                    linkReplace('https://webservices.illinois.edu/webservices/styles/app/blog/sample481_plus.css','link_skin_application');
                    if (typeof(document.getElementById('nav-menu')) != 'undefined' && document.getElementById('nav-menu') != null)
                    {
                        document.getElementById('nav-menu').style.backgroundColor='';
                    }                    
                    //
                    var desktopWrapper = document.getElementById("desktop-wrapper");
                    var dskCont = document.getElementById("dskCont");
                    var mobileWrapper = document.getElementById("mobile-wrapper");
                    if(isResponsive == false)
                    {
                        if (typeof(desktopWrapper) != 'undefined' && desktopWrapper != null)
                        {
                            dskCont.appendChild(contDiv);
                            desktopWrapper.style.display="block";
                            mobileWrapper.style.display="none";
                        }
                    }
                    document.getElementById('mobile-nav').style.display = 'none';
                }
            }
            
            var responsive;
            function setResponsiveState()
            {
                if (typeof(added_by_webtools_is_responsive) != 'undefined' && added_by_webtools_is_responsive != null)
                {
                    responsive = true;
                }
                else
                {
                    var responsive = false;
                    var wrapperDiv = document.createElement("div");
                    wrapperDiv.id = "desktop-wrapper";
                    while (document.body.firstChild)
                    {
                        wrapperDiv.appendChild(document.body.firstChild);
                    }
                    var mobileWrapper = document.createElement("div");
                    mobileWrapper.id = "mobile-wrapper";
                    var mobileContent = document.createElement("div");
                    mobileContent.id = "mobile-content";
                    document.body.appendChild(wrapperDiv);
                    document.body.appendChild(mobileWrapper);
                    mobileWrapper.appendChild(mobileContent);
                }
            }
            
            function onStartUp()
            {   
                setResponsiveState();
                window.onresize = jsUpdateSize;
                setWidths();
                if(mode == 'mobile')
                {
                    //document.getElementById('search-bar').className='open-search';
                }    
            }
            var currentUrl = "" + window.location;
            if(currentUrl.indexOf('scale=true') == -1)
            {
                window.onload = onStartUp;
            }  
